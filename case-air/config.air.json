{
  "time": {
    "0": {
      "onminute": "device/0?log=time:$v"
    }
  },
  "timer": {
    "blink": {
      "type": "LOOP",
      "waittime": "8",
      "pulsetime": "8",
      "cycletime": "20",
      "onon": "device/0?log=blink:on",
      "onoff": "device/0?log=blink:off",
      "description": "Water Timer for testing"
    }
  },
  "pms": {
    "pm25": {
      "loglevel": 0,
      "pinrx": "D2",
      "pintx": "D1",
      "readtime": 300,
      "description": "read out pm25 sensor",
      "onValue": "log/pm?value=$v"
    }
  },
  "log": {
    "pm": {
      "loglevel": 2,
      "filename": "/pmlog.txt",
      "description": "Log values to file"
    }
  }
}