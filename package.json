{
  "name": "homedingnode",
  "version": "1.0.0",
  "description": "Node based emulator of a HomeDing embedded web server",
  "main": "server.js",
  "dependencies": {
    "express": "^4.16.2",
    "serve-favicon": "^2.4.5"
  },
  "devDependencies": {
    "node-sass": "^4.9.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "css": "node-sass iotstyle.scss iotstyle.css --output-style compact",
    "css-watch": "node-sass -w iotstyle.scss iotstyle.css --output-style compact",
    "css-min": "node-sass iotstyle.scss iotstyle.min.css --output-style compressed"

  },
  "keywords": [
    "HomeDing",
    "HomeDingNode",
    "node.js",
    "iot",
    "webserver"
  ],
