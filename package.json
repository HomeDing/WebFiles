{
  "name": "homedingnode",
  "version": "1.0.1",
  "description": "Node based emulator of a HomeDing embedded web server",
  "main": "server.js",
  "dependencies": {
    "debug": "^4.1.1",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "html-minifier": "^3.5.21",
    "node-sass": "^4.10.0",
    "uglify-js": "^3.4.9"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "css": "node-sass iotstyle.scss iotstyle.css --output-style compact",
    "css-watch": "node-sass -w iotstyle.scss iotstyle.css --output-style compact",
    "css-min": "node-sass iotstyle.scss iotstyle.min.css --output-style compressed",
    "min-upload": "html-minifier --collapse-whitespace --remove-comments --minify-css true --minify-js true --quote-character ' upload.htm -o upload.min.htm",
    "min-micro": "uglifyjs micro.js -o micro.min.js",
    "min-ding": "uglifyjs ding.js -o ding.min.js",
    "min-ide": "uglifyjs ding-ide.js -o ding-ide.min.js"
  },
  "keywords": [
    "HomeDing",
    "HomeDingNode",
    "node.js",
    "iot",
    "webserver"
  ],
  "author": "Matthias Hertel",
  "license": "BSD"
}
